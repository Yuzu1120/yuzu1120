import sys

def udedupe_core(s):
    if not s:
        return s
    result = [s[0]]
    for c in s[1:]:
        if c != result[-1]:
            result.append(c)
    return "".join(result)

def main():
    try:
        for line in sys.stdin:
            print(udedupe_core(line.rstrip("\n")))
    except Exception as e:
        print(f"Error: {e}", file=sys.stderr)
        sys.exit(1)

if __name__ == "__main__":
    main()
